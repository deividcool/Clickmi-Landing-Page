{
  "name": "astro-m2dx",
  "version": "0.7.16",
  "description": "Remark plugin to enhance MDX in the scope of Astro site generation",
  "author": "christian@hackyourshack.org",
  "homepage": "https://astro-m2dx.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/christian-hackyourshack/npm.git"
  },
  "license": "MIT",
  "keywords": [
    "remark-plugin",
    "astro",
    "markdown",
    "mdx"
  ],
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "estree-util-visit": "^1.2.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "mdast-util-mdx": "^2.0.0",
    "m2dx-utils": "0.2.6"
  },
  "devDependencies": {
    "@types/estree": "^1.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/mdast": "^3.0.10",
    "@types/node": "^20.3.1",
    "@types/unist": "2.0.6",
    "rimraf": "^3.0.2",
    "tsup": "^7.1.0",
    "tsx": "^3.12.1",
    "unified": "^10.1.2",
    "vfile": "^5.3.6",
    "@internal/utils": "0.0.0",
    "eslint-config-base": "0.0.0",
    "mintest-green": "0.2.3",
    "tsconfig": "0.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "rimraf -rf ./dist/",
    "test": "mintest",
    "test:dev": "tsx watch src/includeDirective/index.spec.ts",
    "test:watch": "tsx watch node_modules/mintest-green/dist/bin.js"
  }
}